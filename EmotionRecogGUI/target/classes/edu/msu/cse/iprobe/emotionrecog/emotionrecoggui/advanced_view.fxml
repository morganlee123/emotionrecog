<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="408.0" prefWidth="507.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.msu.cse.iprobe.emotionrecog.emotionrecoggui.AdvancedReportController">
    <children>
      <BarChart fx:id="barChart" prefHeight="300" prefWidth="500">
        <xAxis >
          <CategoryAxis side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis >
          <NumberAxis side="LEFT" fx:id="yAxis" />
        </yAxis>
      </BarChart>
        <Button fx:id="backReportButton" onAction="#goBackToReport" text="Go Back">
         <font>
            <Font name="Arial" size="13.0" />
         </font></Button>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
