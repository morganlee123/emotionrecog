<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="322.0" prefWidth="508.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.msu.cse.iprobe.emotionrecog.emotionrecoggui.VoiceRecordingController">
    <children>
        <Label text="Press to Record">
         <font>
            <Font name="Arial" size="13.0" />
         </font></Label>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" style="-fx-spacing: 30;">
         <children>
              <Button fx:id="recordButton" onAction="#switchToConfirm" text="Record">
               <font>
                  <Font name="Arial" size="13.0" />
               </font>
            </Button>
            <Button fx:id="goBackPrimaryFromRecordButton" onAction="#switchToPrimary" text="Go Back">
               <font>
                  <Font name="Arial" size="13.0" />
               </font>
            </Button>
         </children>
      </VBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
